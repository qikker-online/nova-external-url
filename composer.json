{
    "name": "qikker-online/external-url",
    "description": "Add external URLs to your laravel nova fields",
    "keywords": [
        "laravel",
        "nova",
        "URL",
        "fields",
        "External Urls"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "QikkerOnline\\ExternalUrl\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QikkerOnline\\ExternalUrl\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
